{
  "name": "tc-message-toolkit",
  "version": "0.0.1",
  "description": "A typescript toolkit to work with TeamCity service messages. Includes a streamable parser as well as the ability to generate synthetic TC log output.",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/tc-message-toolkit.umd.cjs",
  "browser": "./dist/tc-message-toolkit.js",
  "module": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:vitest:clean": "del-cli *.tgz",
    "test": "vitest",
    "dev:browser-demo": "vite --config ./examples/browser-demo/vite.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamscybot/teamcity-service-message-toolkit.git"
  },
  "author": "Adam Thomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamscybot/teamcity-service-message-toolkit/issues"
  },
  "homepage": "https://github.com/adamscybot/teamcity-service-message-toolkit#readme",
  "dependencies": {
    "chalk": "^5.2.0",
    "moo": "^0.5.2",
    "parse-duration": "^1.0.3",
    "ts-custom-error": "^3.3.1",
    "typescript": "^5.1.3",
    "yargs": "^17.7.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/moo": "^0.5.5",
    "@types/node": "^20.1.3",
    "@types/yargs": "^17.0.24",
    "@vitest/coverage-v8": "^0.33.0",
    "del-cli": "^5.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "vite": "^4.3.9",
    "vite-plugin-checker": "^0.6.1",
    "vitest": "^0.33.0"
  }
}
